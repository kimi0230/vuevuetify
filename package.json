{
  "name": "vuevuetify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 8080",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "dev": "vue-cli-service serve --mode dev --port 8080",
    "dev_b": "vue-cli-service build --mode dev",
    "dist": "serve -s dist",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "electron:build_win": "vue-cli-service electron:build --win --x64",
    "electron:build_linux": "vue-cli-service electron:build --linux --x64",
    "local": "vue-cli-service serve --mode local --port 8080",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "prod": "vue-cli-service serve --mode prod --port 80",
    "prod_b": "vue-cli-service build --mode prod",
    "docker_dev": "docker build -f Dockerfile.dev ."
  },
  "main": "background.js",
  "dependencies": {
    "axios": "^0.21.2",
    "core-js": "^3.3.2",
    "crypto-js": "^3.1.9-1",
    "moment": "^2.24.0",
    "serialize-javascript": "^3.1.0",
    "url-loader": "^3.0.0",
    "vue": "^2.6.10",
    "vue-i18n": "^8.15.5",
    "vue-router": "^3.1.3",
    "vuetify": "^2.1.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@mdi/font": "^4.5.95",
    "@mdi/js": "^4.5.95",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.0",
    "@vue/cli-plugin-unit-jest": "^4.0.0",
    "@vue/cli-plugin-vuex": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "deepmerge": "^4.2.2",
    "electron": "^13.6.6",
    "fibers": "^4.0.2",
    "material-design-icons-iconfont": "^5.0.1",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-electron-builder": "^1.4.5",
    "vue-cli-plugin-vuetify": "^2.0.2",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.3.0"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest"
  }
}
